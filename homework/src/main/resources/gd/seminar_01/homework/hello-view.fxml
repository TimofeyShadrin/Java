<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.RowConstraints?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="gd.seminar_01.homework.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <children>
        <GridPane>
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="10" prefWidth="10"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="10" prefWidth="10"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="10" prefWidth="10"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="10" prefWidth="10"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="10" prefWidth="10"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints maxHeight="10" prefHeight="30" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="10" prefHeight="30" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="10" prefHeight="30" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="10" prefHeight="30" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Button maxHeight="-Infinity" maxWidth="-Infinity" mnemonicParsing="false" text="7"/>
            </children>
        </GridPane>
    </children>

    <Label fx:id="welcomeText"/>
    <Button text="Hello!" onAction="#onHelloButtonClick"/>
</VBox>
